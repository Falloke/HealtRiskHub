model d01_influenza {
  id                    Int       @default(autoincrement())
  disease_code          String
  gender                String?
  age_y                 Int?
  nationality           String?
  occupation            String?
  province              String?
  district              String?
  onset_date            String?
  treated_date          String?
  diagnosis_date        String?
  death_date            String?
